{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "components/*": ["src/components/*"],
      "properties/*": ["src/properties/*"]
    }
  },
  "include": ["src/**/*", "App.tsx"]
}
